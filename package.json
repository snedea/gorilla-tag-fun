{
  "name": "gorilla-tag-fun",
  "version": "1.0.0",
  "description": "Gorilla Tag Fun Math Game - An educational math game for 2nd grade students featuring a playful gorilla character in a jungle environment",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:safari": "playwright test --project=webkit",
    "test:e2e:ipad": "playwright test --project=ipad",
    "test:all": "npm run test && npm run test:e2e"
  },
  "keywords": [
    "educational-game",
    "math-game",
    "2nd-grade",
    "phaser",
    "gorilla-tag",
    "browser-game",
    "kids-learning"
  ],
  "author": "Gorilla Tag Fun Team",
  "license": "MIT",
  "dependencies": {
    "phaser": "^3.70.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/jest": "^29.5.10",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "vite": "^5.0.7"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/gorilla-tag-fun.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/gorilla-tag-fun/issues"
  },
  "homepage": "https://yourusername.github.io/gorilla-tag-fun"
}
